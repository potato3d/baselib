include(../_begin_default.pri)

#-----------------------------------------------------------------------------------------------------------------------
# setup as static lib
#-----------------------------------------------------------------------------------------------------------------------
TEMPLATE = lib
CONFIG += staticlib

#------------------------------------------------------------------------------
# source files
#------------------------------------------------------------------------------
HEADERS += $$files(algebra/*.h)
SOURCES += $$files(algebra/*.cpp)
HEADERS += $$files(search/*.h)
SOURCES += $$files(search/*.cpp)
HEADERS += $$files(select/*.h)
SOURCES += $$files(select/*.cpp)
HEADERS += $$files(sort/*.h)
SOURCES += $$files(sort/*.cpp)
HEADERS += $$files(util/*.h)
SOURCES += $$files(util/*.cpp)

#-----------------------------------------------------------------------------------------------------------------------
# output configuration
#-----------------------------------------------------------------------------------------------------------------------
DESTDIR = $${ROOT_DIR}/libraries

#------------------------------------------------------------------------------
# reuse default project setup
#------------------------------------------------------------------------------
include(../_end_default.pri)
